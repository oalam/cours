<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <jsp-config>
        <taglib>
            <taglib-uri>monTag</taglib-uri>
            <taglib-location>/WEB-INF/tlds/montaglib.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>scTag</taglib-uri>
            <taglib-location>/WEB-INF/tlds/3scTags.tld</taglib-location>
        </taglib>
        <jsp-property-group>
            <description>		</description>
            <display-name>headerProperty</display-name>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/header.jspf</include-prelude>
        </jsp-property-group>
    </jsp-config>
</web-app>
